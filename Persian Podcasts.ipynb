   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import spotipy\n",
    "from spotipy.oauth2 import SpotifyClientCredentials"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Auth\n",
    "sp = spotipy.Spotify(auth_manager=SpotifyClientCredentials(client_id=\"\",\n",
    "                                                           client_secret=\"\"))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Recieving Data from Spotify\n",
    "show = sp.search(q='پادکست', type='show', market='US', offset=0, limit=50)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "names = []\n",
    "description = []\n",
    "publisher = []\n",
    "total_episodes = []\n",
    "external_urls = []\n",
    "for i in range(40):\n",
    "    show = sp.search(q='پادکست', type='show', market='US', offset=i*50, limit=50)\n",
    "    show = show['shows']['items']\n",
    "    for e in show:\n",
    "        names.append(e['name'])\n",
    "        description.append(e['description'])\n",
    "        publisher.append(e['publisher'])\n",
    "        total_episodes.append(e['total_episodes'])\n",
    "        external_urls.append(e['external_urls']['spotify'])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "names = pd.Series(names)\n",
    "description = pd.Series(description)\n",
    "publisher = pd.Series(publisher)\n",
    "total_episodes = pd.Series(total_episodes)\n",
    "external_urls = pd.Series(external_urls)\n",
    "#pd.set_option('display.max_rows', None)\n",
    "#pd.set_option('display.max_colwidth', None)"
   ]
  },
